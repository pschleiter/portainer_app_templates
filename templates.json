{
  "version": "2",
  "templates": [
    {
      "type": 1,
      "title": "PostgreSQL",
      "description": "The most advanced open-source database",
      "categories": ["database"],
      "platform": "linux",
      "logo": "https://portainer-io-assets.sfo2.digitaloceanspaces.com/logos/postgres.png",
      "image": "postgres:latest",
      "restart_policy": "always",
      "env": [
        {
          "name": "POSTGRES_USER",
          "label": "Superuser"
        },
        {
          "name": "POSTGRES_PASSWORD",
          "label": "Superuser password"
        }
      ],
      "ports": ["5432/tcp"],
      "volumes": [
        {
          "container": "/var/lib/postgresql/data"
        }
      ]
    },
    {
      "type": 1,
      "title": "Nextcloud",
      "description": "Content collaboration platform.",
      "categories": ["fileshare"],
      "platform": "linux",
      "logo": "https://upload.wikimedia.org/wikipedia/commons/thumb/6/60/Nextcloud_Logo.svg/1920px-Nextcloud_Logo.svg.png",
      "image": "nextcloud:latest",
      "restart_policy": "on-failure",
      "env": [
        {
          "name": "POSTGRES_Host",
          "label": "Postgres Database Host"
        },
        {
          "name": "POSTGRES_DB",
          "label": "Postgres Database Name"
        },
        {
          "name": "POSTGRES_USER",
          "label": "Postgres Database User"
        },
        {
          "name": "POSTGRES_PASSWORD",
          "label": "Postgres Database Password"
        }
      ],
      "ports": ["5432/tcp"],
      "volumes": [
        {
          "container": "/var/www/html"
        }
      ]
    }
  ]
}
