{
  "version": "2",
  "templates": [
    {
      "type": 1,
      "title": "PostgreSQL",
      "description": "The most advanced open-source database",
      "categories": ["database"],
      "platform": "linux",
      "logo": "https://portainer-io-assets.sfo2.digitaloceanspaces.com/logos/postgres.png",
      "image": "postgres:latest",
      "restart_policy": "always",
      "env": [
        {
          "name": "POSTGRES_USER",
          "label": "Superuser"
        },
        {
          "name": "POSTGRES_PASSWORD",
          "label": "Superuser password"
        }
      ],
      "volumes": [
        {
          "container": "/var/lib/postgresql/data",
          "bind": "postgres_data"
        }
      ],
      "labels": [
        {
          "name": "traefik.enable",
          "value": "false"
        }
      ]
    },
    {
      "type": 1,
      "title": "Nextcloud",
      "description": "Content collaboration platform.",
      "categories": ["fileshare"],
      "platform": "linux",
      "logo": "https://upload.wikimedia.org/wikipedia/commons/thumb/6/60/Nextcloud_Logo.svg/1920px-Nextcloud_Logo.svg.png",
      "image": "nextcloud:latest",
      "restart_policy": "on-failure",
      "volumes": [
        {
          "container": "/var/www/html",
          "bind": "nextcloud_data"
        }
      ],
      "labels": [
        {
          "name": "traefik.enable",
          "value": "false"
        },
        {
          "name": "traefik.http.routers.nextcloud.entrypoints",
          "value": "websecure"
        },
        {
          "name": "traefik.http.routers.nextcloud.rule",
          "value": "Host(<hostname>)"
        },
        {
          "name": "traefik.http.routers.nextcloud.service",
          "value": "nextcloud"
        },
        {
          "name": "traefik.http.services.nextcloud.loadbalancer.server.port",
          "value": "80"
        }
      ]
    },
    {
      "type": 1,
      "title": "Pi-hole",
      "description": "DNS server",
      "categories": ["network"],
      "platform": "linux",
      "logo": "https://upload.wikimedia.org/wikipedia/en/thumb/1/15/Pi-hole_vector_logo.svg/1024px-Pi-hole_vector_logo.svg.png",
      "image": "pihole/pihole",
      "restart_policy": "always",
      "env": [
        {
          "name": "TZ",
          "default": "Europa/Berlin"
        },
        {
          "name": "CORS_HOSTS"
        }
      ],
      "volumes": [
        {
          "container": "/etc/pihole",
          "bind": "pihole_data"
        },
        {
          "container": "/etc/dnsmasq.d"
        }
      ],
      "labels": [
        {
          "name": "traefik.enable",
          "value": "false"
        },
        {
          "name": "traefik.http.routers.pihole.entrypoints",
          "value": "websecure"
        },
        {
          "name": "traefik.http.routers.pihole.rule",
          "value": "Host(<hostname>) && RoutePrefix(`/admin`)"
        },
        {
          "name": "traefik.http.routers.pihole.service",
          "value": "pihole"
        },
        {
          "name": "traefik.http.services.pihole.loadbalancer.server.port",
          "value": "80"
        }
      ]
    }
  ]
}
